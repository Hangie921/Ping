extends ../default/layout

block main_menu
	include ../mixin/menu.jade
block content
	#content
		h6 user type is #{mem_type} #{_id} #{pwd}
		case pwd
			when "company"
				h1 Welcome to Company Dashboard
			when "talent"
				h1 Welcome to Talent Dashboard
			when "walter"
				h1 Welcome to Talent Dashboard
			default
				h1 Hi Stranger , you don't have a user_type
		#info
			h1 Info
			.brief_profile
				ul
					li Pic: I am pic
					li Name: I am your father
					li Industries: Star Wars
block form
	#form_container
		#logout_form
			form(method="post",action='logout')
				input(type='submit',value='bye')
		#show_users
			a(href='/users') show users





	